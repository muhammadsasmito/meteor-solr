{
  "public": {
    "search": {
      "scheme": "http",
      "host": "localhost",
      "port": 8983,
      "core": "testcore",
      "suggesterUrl": "/suggesthandler2",
      "limitFields": [
        "id",
        "bahasa",
		"kota_terbit",
		"id_isbn",
		"kontributor",
		"edisi",
		"jml_hlm",
		"bibliografi",
        "klasifikasi",
        "judul",
        "pengarang",
        "penerbit",
        "th_terbit",
        "subyek",
        "file2"
      ],
      "flatFacets": {
        "doctype": {
          "limit": 10,
          "sort": "count"
        },
        "pengarang": {
          "limit": 10,
          "sort": "count"
        },
        "ml_entity_location": {
          "limit": 10,
          "sort": "count"
        },
        "ml_entity_organization": {
          "limit": 10,
          "sort": "count"
        },
        "ml_keyword": {
          "limit": 10,
          "sort": "count"
        }
      },
      "hierarchicalFacets": {
        "source": {
          "limit": 100,
          "sort": "index"
        },
        "th_terbit": {
          "limit": 100,
          "sort": "index"
        }
      },
      "customFacetLabels": {
        "doctype": {
          "html": "HTML",
          "pdf": "PDF",
          "other": "Other",
          "configuration": "Configuration",
          "presentation": "Presentation",
          "text": "Text",
          "wordprocessor": "Wordprocessor",
          "archive": "Archive",
          "spreadsheet": "Spreadsheet"
        }
      }
    },
    "analyticsControl": {
      "loggingEnabled": true,
      "containers": [ "cloud", "database" ]
    },
    "analyticsSettings": {
      "Google Analytics": {
        "trackingId": "UA-1885741-2"
      }
    }
  },
  "private": {
    "analytics": {
      "jobs": {
        "export": {
          "enabled": false,
          "schedule": "every 1 hour",
          "endpointUrl": "https://someurl.xyz/analytics-capture"
        },
        "cleanup": {
          "enabled": false,
          "schedule": "on the first day of the month",
          "monthRetainCount": "1"
        }
      }
    }
  }
}
